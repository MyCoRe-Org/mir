<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<actionmappings>
  <collection name="simple">
    <action action="search">
      <when condition="true" url="/content/search/simple.xed" />
    </action>
  </collection>
  <collection name="complex">
    <action action="search">
      <when condition="(group = admin) or (group = editor)" url="/content/search/complex_intern.xml" />
      <when condition="true" url="/content/search/complex.xed" />
    </action>
  </collection>
  <collection name="personindex">
    <action action="browse">
      <when condition="(group = admin) or (group = editor)" url="/servlets/solr/personindex?XSL.Style=person&amp;terms.regex=[a|%C3%A4|%C3%84].*" />
      <when condition="true" url="/servlets/solr/personindexp?XSL.Style=person&amp;terms.regex=[a|%C3%A4|%C3%84].*" />
    </action>
  </collection>
  <collection name="mods">
    <action action="create">
      <when condition="(group = admin) or (group = editor)" url="/editor/editor-admins.xed" />
      <when condition="group = submitter" url="/content/publish/index.xml" />
      <when condition="true" url="/authorization/new-author.xed" />
    </action>
    <action action="update">
      <when condition="(group = admin) or (group = editor)" url="/servlets/MCRLockServlet?url=/editor/editor-admins.xed" />
      <when condition="(status != published) and (status != review)" url="/servlets/MCRLockServlet?url=/editor/editor-dynamic.xed?genre=" /> <!-- TODO: genre as param -->
    </action>
    <action action="update-xml">
      <when condition="(group = admin)" url="/servlets/MCRLockServlet?url=/editor_form_commit-mods-all.xml" />
      <when condition="(status != published) and (status != review)" url="/servlets/MCRLockServlet?url=/editor_form_commit-mods-all.xml" />
    </action>
  </collection>

